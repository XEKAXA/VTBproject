<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="VTBproject.MainPage">
   
    <VerticalStackLayout>
        <WebView 
            Source="https://www.openstreetmap.org/export#map=13/55.7503/37.6366"
            x:Name="webView_map"
            HeightRequest="450"/>
        <HorizontalStackLayout HorizontalOptions="Center">
            <Button
                x:Name="btn_LoadData"
                Text="Get Data"
                SemanticProperties.Hint="Get data from files"
                Clicked="btn_LoadDataClicked"
                HorizontalOptions="Center"
                IsEnabled="True"/>
        </HorizontalStackLayout>

    </VerticalStackLayout>

</ContentPage>
